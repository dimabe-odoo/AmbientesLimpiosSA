<odoo>
    <data>
        <record id="view_stock_move_line_detailed_operation_tree_inherit" model="ir.ui.view">
            <field name="name">Detailed Operations Inherit</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']"
                       position="after">
                    <field name="is_loteable" invisible="1"/>
                    <field name="supplier_lot" attrs="{'readonly':[('is_loteable','!=',True)]}"/>
                </xpath>
                <xpath expr="//field[@name='lot_name']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>    
            </field>
        </record>

        <record id="view_stock_picking_tree" model="ir.ui.view">
            <field name="name">Stock Picking Tree Inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="l10n_latam_document_type_id" widget="many2onebutton"/>
                </xpath>
            </field>
        </record>

        <record id="view_stock_picking_search" model="ir.ui.view">
            <field name="name">Stock Picking Search Inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_internal_search"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="l10n_latam_document_type_id"/>
                    <filter name="Factura Electronica" domain="[('l10n_latam_document_type_id.code','=',33)]"/>
                    <filter name="Boleta Electronica" domain="[('l10n_latam_document_type_id.code','=',39)]"/>
                </xpath>
            </field>
        </record>
    </data> 

</odoo>