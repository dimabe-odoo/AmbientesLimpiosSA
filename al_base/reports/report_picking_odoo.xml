<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="report_picking_inherit" inherit_id="stock.report_picking">
            <xpath expr="//div[@class='page']//table[1]//tbody//tr//td[6]//span" position="replace">
                <t t-if="move.product_id.al_dun">
                    <span>
                        <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move.product_id.al_dun, 600, 100)"
                             style="width:300px;height:50px;"/>
                    </span>
                </t>
                <t t-else="">
                    <span>
                        <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move.product_id.al_sku, 600, 100)"
                         style="width:300px;height:50px;"/>
                    </span>
                </t>
            </xpath>
        </template>

    </data>
</odoo>